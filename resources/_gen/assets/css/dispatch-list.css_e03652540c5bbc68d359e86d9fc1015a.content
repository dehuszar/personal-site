.dispatches-list  {
	margin: 0;
	padding: 0;
}

.dispatches-list li {
	list-style: none;
}

.dispatches-list li img {
	max-height: 100%;
	max-width: 100%;
}

.dispatches-list li date {
	display: block;
	font-size: 1.4em;
	margin: 0 0 .5em 0;
}

.dispatches-list h3 {
  font-size: 4em;
  margin: 0.5em 0;
}

.dispatches-list p {
	font-size: 1.8em;
}

@media (min-width: 1280px) {
	.dispatches-list {
		grid-column: 5 / span 8;
		grid-row: 3 / span 15;
		grid-template-columns: subgrid;
		grid-template-rows: subgrid;
		grid-gap: subgrid;
		margin: 0;
		padding: 0;
	}

	.dispatches-list li,
	.dispatches-list li a {
		grid-column: 1 / span 8;
		grid-row: 1 / span 4;
    grid-template-columns: repeat(8, minmax(0, 1fr));
    grid-template-rows: repeat(4, 3em);
    grid-gap: 1em 3em;
    display: grid;
	}

  .dispatches-list li + li {
		grid-row: 1 / span 5;
    grid-template-rows: repeat(5, 3em);
	}

  .dispatches-list li + li a {
    border-top: 1px solid;
    grid-row: 2 / span 4;
    margin-top: -2em;
    padding-top: 2em;
  }

	.dispatches-list li figure {
    border-radius: 5%/10%;
		grid-column: 6 / span 3;
		grid-row: 1 / span 4;
    margin: 0;
		max-height: 100%;
		max-width: 100%;
    overflow: hidden;
    text-align: center;
	}

	.dispatches-list li h3 {
		grid-column: 1 / span 5;
		grid-row: 1 / span 1;
		margin: 0;
		font-size: 2em;
	}

	.dispatches-list li date {
		grid-column: 1 / span 5;
		grid-row: 4 / span 1;
	}

	.dispatches-list li p {
		font-size: 1.4em;
		grid-column: 1 / span 5;
		grid-row: 2 / span 2;
		margin: 0;
	}

	.dispatches-list ~ aside {
		grid-column: 2 / span 3;
		grid-row: 3 / span 4;
	}
}

