{
    {{ with  .OutputFormats.Get "json" -}}
    "datapermalink": "{{ .Permalink }}",
    {{ end }}
    "body": {{ trim ( .Content | jsonify | safeJS ) "\n" }},
    "endYear": "{{ .Params.end_year }}",
    "homepage": "{{ .Params.homepage }}",
    "location": "{{ .Params.location }}",
    "logo": "{{ .Params.logo }}",
    "permalink": "{{ .RelPermalink }}",
    "slug": "{{ .Params.slug }}",
    "startYear": "{{ .Params.start_year }}",
    "tags": [{{ range $index, $taxonomy := .Params.tags }}{{ if $index }}, {{ end }}"{{ $taxonomy }}"{{ end }}],
    "title": "{{ .Title }}",
    "type": "{{ .Type }}"
}